{
	"expo": {
		"name": "Expense List",
		"description": "App to manage your expenses",
		"slug": "expenseList",
		"owner": "derfelixplayer2",
		"privacy": "hidden",
		"platforms": ["android"],
		"githubUrl": "https://github.com/DerFelixPlayer2/ExpenseList.git",
		"orientation": "portrait",
		"userInterfaceStyle": "dark",
		"backgroundColor": "#000000",
		"primaryColor": "#292938",
		"icon": "./assets/icon.png",
		"androidNavigationBar": {
			"backgroundColor": "#292938"
		},
		"updates": {
			"enabled": false
		},
		"assetBundlePatterns": ["**/*"],
		"splash": {
			"backgroundColor": "#292938",
			"resizeMode": "contain",
			"image": "./assets/splash.png"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "de.lolgamr17.expenselist"
		},
		"android": {
			"package": "de.lolgamr17.expenselist",
			"softwareKeyboardLayoutMode": "pan",
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive_icon.png",
				"backgroundColor": "#cf4a1d"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"lang": "en"
		}
	}
}
